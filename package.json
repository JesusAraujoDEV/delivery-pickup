{
  "name": "delivery-pickup",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "echo \"No linter configured\"",
    "db:test": "node src/scripts/test-aiven.js",
    "migrations:run": "sequelize-cli db:migrate --config src/db/config.cjs --migrations-path src/db/migrations",
    "migrations:revert": "sequelize-cli db:migrate:undo --config src/db/config.cjs --migrations-path src/db/migrations",
    "migrations:revert:all": "sequelize-cli db:migrate:undo:all --config src/db/config.cjs --migrations-path src/db/migrations",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "test:integration": "node --test"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-stringify": "^6.5.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.12.3",
    "metrex": "^0.5.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.3"
  }
}
